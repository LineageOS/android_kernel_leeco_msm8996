		reg = <0>;
		status = "ok";
		spi-max-frequency = <19200000>;
		nxp,p61-irq = <&tlmm 131 0>;
		interrupt-parent = <&tlmm>;
		interrupts = <131 0>;
		nxp,p61-rst = <&tlmm 130 0>;
	};
};
